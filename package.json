{
  "name": "lkt-tabs",
  "version": "1.3.0",
  "description": "",
  "main": "src/index.ts",
  "module": "src/index.ts",
  "files": [
    "dist/*",
    "src/**/*.vue",
    "theme/**/*.css"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/lkt-tabs.es.js",
      "require": "./dist/lkt-tabs.umd.js"
    },
    "./dist/style.css": "./dist/lkt-tabs.css",
    "./theme": "./theme/default.css",
    "./theme/default": "./theme/default.css"
  },
  "types": "./dist/types/index.d.ts",
  "scripts": {
    "serve": "vite",
    "build": "vite build && vue-tsc --emitDeclarationOnly --project tsconfig.json && mv dist/src dist/types && rm dist/README.md.d.ts && rm dist/vite.config.d.ts",
    "build2": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test-coverage": "vitest run --coverage"
  },
  "author": "Antonio Ibáñez",
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "@babel/types": "^7.23.6",
    "@types/node": "^20.11.19",
    "@types/rollup": "^0.54.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "rollup": "^4.34.8",
    "typescript": "^5.7.3",
    "vite": "^6.1.1",
    "vue": "^3.3.0",
    "vue-tsc": "^2.2.0"
  },
  "peerDependencies": {
    "lkt-session": "^1.0.6",
    "lkt-string-tools": "^1.1.0",
    "lkt-vue-kernel": "^1.0.51",
    "vue": "^3.0.5"
  }
}
